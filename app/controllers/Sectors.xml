<?xml version="1.0" encoding="utf-8"?>
<dataController name="Sectors" conflictDetection="overwriteChanges" label="Sectors" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"Sectors"."ID" "ID"
	,"Sectors"."truckCost" "TruckCost"
	,"Sectors"."name" "Name"
	,"Sectors"."tank" "Tank"
	,"Sectors"."sortOrder" "SortOrder"
	,"Sectors"."maxStops" "MaxStops"
	,"Sectors"."maxBMEs" "MaxBmes"
	,"Sectors"."avgDaysFromOrderToShipped" "AvgDaysFromOrderToShipped"
	,"Sectors"."truckCostMultiplier" "TruckCostMultiplier"
from "dbo"."Sectors" "Sectors"
]]></text>
    </command>
  </commands>
  <fields>
