    <field name="SortOrder" type="Int64" label="sort Order" showInSummary="true" />
    <field name="MaxStops" type="Int64" label="max Stops" showInSummary="true" />
    <field name="MaxBmes" type="Double" label="max BM Es" />
    <field name="AvgDaysFromOrderToShipped" type="Int64" label="avg Days From Order To Shipped" />
    <field name="TruckCostMultiplier" type="Double" dataFormatString="c" label="truck Cost Multiplier" />
  </fields>
